package entities;

import java.util.Scanner;

public class Matrix {
	Scanner sc = new Scanner(System.in);
	int n, col, lin, matriz[][];

	public Matrix(int n) {
		super();
		this.n = n;
	}
	
	public void buildmatriz() {
		int matriz[][] = new int[n][n];
		for(int i=0; i<n;i++) {
			for (int j=0; j<n; j++) {
				matriz[i][j] = sc.nextInt();
			}
		}
		
	}
	
	
}
