package entities;

import java.util.Scanner;

public class Matrix {
	Scanner sc = new Scanner(System.in);
	int dimension, col, lin, matriz[][];

	public Matrix(int dimension, int[][] matriz) {
		super();
		this.dimension = dimension;
		this.matriz = matriz;
	}
	
	void buildmatriz() {
		Integer n = sc.nextInt();
		int matriz[][] = new int[n][n];
		for(int i=0; i<n;i++) {
			for (int j=0; j<n; j++) {
				matriz[i][j] = sc.nextInt();
			}
		}
		
	}
}
